<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Car.TestCar">
    <Grid RowSpacing="0">
        <StackLayout Orientation="Vertical">
            <Entry x:Name="IPEntry" Placeholder="IP Address" Text="192.168.210.102"></Entry>
            <Button x:Name="Start" VerticalOptions="Center" Text="Start" Clicked="Start_Clicked"></Button>
        </StackLayout>
    </Grid>
</ContentPage>